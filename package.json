  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "dependencies": {
    "expo": "~49.0.13",
    "expo-status-bar": "~1.6.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-native": "0.72.5",
    "react-native-web": "~0.19.6"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@expo/webpack-config": "^18.1.3"
  },
  "scripts": {
    "app:web": "yarn workspace @onekeyhq/web start",
